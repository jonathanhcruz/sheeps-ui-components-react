{
  "name": "package-template",
  "private": false,
  "description": "package-template-description",
  "version": "1.0.0",
  "tags": [
    "react",
    "typescript",
    "ui",
    "components",
    "library"
  ],
  "type": "module",
  "license": "MIT",
  "author": "Jonathan Cruz",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/sheep-ui-components.ts",
  "module": "./dist/sheep-ui-components.es.ts",
  "types": "./dist/sheep-ui-components.d.ts",
  "exports": {
    ".": {
      "import": "./dist/sheep-ui-components.es.ts",
      "require": "./dist/sheep-ui-components.ts"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build": "tsc && vite build",
    "test": "vitest",
    "test-coverage": "vitest --coverage",
    "test-watch": "vitest --watch",
    "test-ui": "vitest --ui --watch --coverage"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.1.0",
    "@storybook/addon-interactions": "^7.1.0",
    "@storybook/addon-links": "^7.1.0",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.1.0",
    "@storybook/react": "^7.1.0",
    "@storybook/react-vite": "^7.1.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-v8": "^0.33.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.13",
    "jsdom": "^22.1.0",
    "storybook": "^7.1.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.33.0"
  }
}
